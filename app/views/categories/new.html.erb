<%= render 'shared/page_header', title: 'Create a Neighborhood' %>

<%= render 'shared/error_display', obj: @category %>

<div class = 'well'>
  <%= form_for @category do |f| %>
    <%= f.label 'Name' %>
    <%= f.text_field :name, class: 'span5' %>
      </br>
    <%= f.submit 'Create Neighborhood', class: 'btn btn-success' %>
  <% end %>
</div>