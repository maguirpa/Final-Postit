<%= render 'shared/page_header', title: 'Register User' %>

<%= render 'shared/error_display', obj: @user %>


<div class = 'well' >
  <%= form_for @user do |f| %>
    <%= f.label 'Username' %>
    <%= f.text_field :username %>
    </br>
    <%= f.label 'Password' %>
    <%= f.password_field :password %>
    </br>
    <%= f.submit 'Create Account', class: 'btn btn-success' %>
  <% end %>
</div>